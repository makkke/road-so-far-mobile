{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "node": true,
  },
  "ecmaFeatures": {
    "jsx": true,
    "es6": true,
    "classes": true,
  },
  "globals": {
    "fetch": true,
  },
  "rules": {
    "semi": [2, "never"],
    "comma-dangle": [1, "always-multiline"],
    "max-len": [1, 110, 4],
    "arrow-body-style": [0],

    "react/jsx-filename-extension": [0]
  },
}